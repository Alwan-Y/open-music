{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "indent": ["error", 4],
        "allowTemplateLiterals": 0,
        "import/no-extraneous-dependencies": 0
    }
}